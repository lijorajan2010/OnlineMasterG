        }

        p {
            font-size: 14px;
            margin-bottom: 10px;
        }

        .instructionHead {
            border-bottom: 1px solid #ddd;
        }

        .instructionFoot {
            border-top: 1px solid #ddd;
            padding-top: 9px;
        }

        .modal-backdrop {
            display: none;
        }

        .modal-header {
            display: block;
        }

        .modal-open .modal {
            background: rgba(0, 0, 0, 0.72);
