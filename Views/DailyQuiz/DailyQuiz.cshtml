                display: none
            }
        }

        @@media(min-width:768px) {
            .hidden-md, .hidden-lg {
                display: none;
            }

            .dropdown-menu {
                position: relative;
                display: block;
                float: none;
                width: 100%;
            }

            li.nav-item {
                display: inline-block;
            }

            #question, .hide_questions {
                display: none
            }

            #SearchParameters {
